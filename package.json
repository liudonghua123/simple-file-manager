{
  "name": "simple-file-manager-vue",
  "version": "0.1.0",
  "main": "dist/simple-file-manager-vue.common.js",
  "unpkg": "dist/simple-file-manager-vue.umd.min.js",
  "browser": {
    "./sfc": "src/components/SimpleFileManager.vue"
  },
  "author": {
    "name": "Donghua Liu",
    "email": "liudonghua123@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/liudonghua123/simple-file-manager-vue/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name simple-file-manager-vue src/components/SimpleFileManager.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue-context": "^5.1.0"
  },
  "peerDependencies": {
    "ant-design-vue": "^1.2.4",
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "vue-cli-plugin-ant-design": "^1.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-dangle": [
        "off",
        {
          "arrays": "always",
          "objects": "always",
          "imports": "never",
          "exports": "never",
          "functions": "never"
        }
      ],
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ]
}
